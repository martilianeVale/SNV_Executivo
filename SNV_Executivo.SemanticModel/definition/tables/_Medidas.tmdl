	measure 'Meta Desafio ( Leads D-1 proporcional DU e FDS | Periodo 1 | N )' =
			
			VAR lastDayOfMonth = EOMONTH(MAX('d_Calendário'[Date]),-1)
			VAR tblWeek = FILTER(
			    ALL('d_CalendárioApoio'),
			    'd_CalendárioApoio'[Mês]= MONTH(lastDayOfMonth) && 'd_CalendárioApoio'[Ano]= YEAR(lastDayOfMonth) && 'd_CalendárioApoio'[DiaUtil]=1
			)
			VAR tblWeekend = FILTER(
			    ALL('d_CalendárioApoio'),
			    'd_CalendárioApoio'[Mês]=MONTH(lastDayOfMonth) && 'd_CalendárioApoio'[Ano]= YEAR(lastDayOfMonth) && 'd_CalendárioApoio'[DiaUtil]=0
			)
			VAR weekendFactor = DIVIDE(
			    DIVIDE(
			        CALCULATE(
			            [Leads ( Total | Período2 | N )],
			            tblWeekend
			        ),
			        COUNTROWS(tblWeekend),
			        0
			    ),
			    DIVIDE(
			        CALCULATE(
			            [Leads ( Total | Período2 | N )],
			            tblWeek
			        ),
			        COUNTROWS(tblWeek),
			        0
			    ),
			    0
			)
			VAR monthBase = MONTH(MAX('d_Calendário'[Date]))
			VAR yearBase = YEAR(MAX('d_Calendário'[Date]))
			VAR tblThisMonth = FILTER(
			    ALL('d_CalendárioApoio'),
			    'd_CalendárioApoio'[Mês]=monthBase && 'd_CalendárioApoio'[Ano]=yearBase
			)
			VAR tblPartialMonth = FILTER(
			    ALLSELECTED('d_Calendário'),
			    'd_Calendário'[Mês]=monthBase && 'd_Calendário'[Ano]=yearBase
			)
			VAR fullMonthProp = COUNTROWS(FILTER(tblThisMonth, [DiaUtil]=1)) + (COUNTROWS(FILTER(tblThisMonth,[DiaUtil]=0)) * weekendFactor)
			VAR partialMonthProp = COUNTROWS(FILTER(tblPartialMonth, [DiaUtil]=1)) + (COUNTROWS(FILTER(tblPartialMonth,[DiaUtil]=0)) * weekendFactor)
			
			RETURN DIVIDE(
			    partialMonthProp,
			    fullMonthProp,
			    0
			)*[Meta Desafio ( Leads | Periodo | N )]
		formatString: #,0
		lineageTag: 35a9ceba-afaa-433d-8288-d2c8b2094823

		changedProperty = FormatString

	measure 'Meta Diária Desafio ( Proporcional DU/FDS | Intervalo | N )' =
			
			
			VAR weekdayOrWeekend = CALCULATE(
			    MAX('d_Calendário'[SemanaOrFDS]),
			    FILTER(
			        'd_Calendário',
			        MAX('d_Calendário'[Date])
			    )
			)
			VAR lastDayOfMonth = EOMONTH(MAX('d_Calendário'[Date]),-1)
			VAR tblWeek = FILTER(
			    ALLSELECTED('d_Calendário'),
			    'd_Calendário'[Mês]= MONTH(lastDayOfMonth) && 'd_Calendário'[SemanaOrFDS]="Semana"
			)
			VAR tblWeekend = FILTER(
			    ALLSELECTED('d_Calendário'),
			    'd_Calendário'[Mês]=MONTH(lastDayOfMonth) && 'd_Calendário'[SemanaOrFDS]="FDS"
			)
			VAR weekendFactor = DIVIDE(
			    DIVIDE(
			        CALCULATE(
			            SUM(f_Leads[Leads]),
			            USERELATIONSHIP('d_Calendário'[Date], f_Leads[Date]),
			            tblWeekend
			        ),
			        COUNTROWS(tblWeekend),
			        0
			    ),
			    DIVIDE(
			        CALCULATE(
			            SUM(f_Leads[Leads]),
			            USERELATIONSHIP('d_Calendário'[Date], f_Leads[Date]),
			            tblWeek
			        ),
			        COUNTROWS(tblWeek),
			        0
			    ),
			    0
			)
			VAR monthBase = MONTH(MAX('d_Calendário'[Date]))
			VAR yearBase = YEAR(MAX('d_Calendário'[Date]))
			VAR tblThisMonth = FILTER(
			    ALL('d_CalendárioApoio'),
			    'd_CalendárioApoio'[Mês]=monthBase && 'd_CalendárioApoio'[Ano]=yearBase
			)
			VAR fullMonthProp = COUNTROWS(FILTER(tblThisMonth, [SemanaOrFDS]="Semana")) + (COUNTROWS(FILTER(tblThisMonth,[SemanaOrFDS]="FDS")) * weekendFactor)
			RETURN DIVIDE(
			    IF(
			        weekdayOrWeekend="FDS",
			        weekendFactor,
			        1
			    ),
			    fullMonthProp,
			    0
			)*_Medidas[Meta Desafio ( Leads | Periodo | N )]
