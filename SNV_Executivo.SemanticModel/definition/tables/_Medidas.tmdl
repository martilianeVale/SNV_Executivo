	measure 'Meta ( Leads D-1 proporcional DU e FDS | Periodo 1 | N )' =
			
			VAR lastDayOfMonth = EOMONTH(MAX('d_Calendário'[Date]),-1)
			VAR tblWeek = FILTER(
			    ALL('d_CalendárioApoio'),
			    'd_CalendárioApoio'[Mês]= MONTH(lastDayOfMonth) && 'd_CalendárioApoio'[Ano]= YEAR(lastDayOfMonth) && 'd_CalendárioApoio'[DiaUtil]=1
			)
			VAR tblWeekend = FILTER(
			    ALL('d_CalendárioApoio'),
			    'd_CalendárioApoio'[Mês]=MONTH(lastDayOfMonth) && 'd_CalendárioApoio'[Ano]= YEAR(lastDayOfMonth) && 'd_CalendárioApoio'[DiaUtil]=0
			)
			VAR weekendFactor = DIVIDE(
			    DIVIDE(
			        CALCULATE(
			            [Leads ( Total | Período2 | N )],
			            tblWeekend
			        ),
			        COUNTROWS(tblWeekend),
			        0
			    ),
			    DIVIDE(
			        CALCULATE(
			            [Leads ( Total | Período2 | N )],
			            tblWeek
			        ),
			        COUNTROWS(tblWeek),
			        0
			    ),
			    0
			)
			VAR monthBase = MONTH(MAX('d_Calendário'[Date]))
			VAR yearBase = YEAR(MAX('d_Calendário'[Date]))
			VAR tblThisMonth = FILTER(
			    ALL('d_CalendárioApoio'),
			    'd_CalendárioApoio'[Mês]=monthBase && 'd_CalendárioApoio'[Ano]=yearBase
			)
			VAR tblPartialMonth = FILTER(
			    ALLSELECTED('d_Calendário'),
			    'd_Calendário'[Mês]=monthBase && 'd_Calendário'[Ano]=yearBase
			)
			VAR fullMonthProp = COUNTROWS(FILTER(tblThisMonth, [DiaUtil]=1)) + (COUNTROWS(FILTER(tblThisMonth,[DiaUtil]=0)) * weekendFactor)
			VAR partialMonthProp = COUNTROWS(FILTER(tblPartialMonth, [DiaUtil]=1)) + (COUNTROWS(FILTER(tblPartialMonth,[DiaUtil]=0)) * weekendFactor)
			
			RETURN DIVIDE(
			    partialMonthProp,
			    fullMonthProp,
			    0
			)*[Meta ( Leads | Periodo | N )]
