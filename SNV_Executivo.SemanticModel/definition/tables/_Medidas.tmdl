	measure 'Meta Diária ( Proporcional DU/FDS | Intervalo | N )' =
			
			
			VAR weekdayOrWeekend = CALCULATE(
			    MAX('d_Calendário'[SemanaOrFDS]),
			    FILTER(
			        'd_Calendário',
			        MAX('d_Calendário'[Date])
			    )
			)
			VAR lastDayOfMonth = EOMONTH(MAX('d_Calendário'[Date]),-1)
			VAR tblWeek = FILTER(
			    ALLSELECTED('d_Calendário'),
			    'd_Calendário'[Mês]= MONTH(lastDayOfMonth) && 'd_Calendário'[SemanaOrFDS]="Semana"
			)
			VAR tblWeekend = FILTER(
			    ALLSELECTED('d_Calendário'),
			    'd_Calendário'[Mês]=MONTH(lastDayOfMonth) && 'd_Calendário'[SemanaOrFDS]="FDS"
			)
			VAR weekendFactor = DIVIDE(
			    DIVIDE(
			        CALCULATE(
			            SUM(f_Leads[Leads]),
			            USERELATIONSHIP('d_Calendário'[Date], f_Leads[Date]),
			            tblWeekend
			        ),
			        COUNTROWS(tblWeekend),
			        0
			    ),
			    DIVIDE(
			        CALCULATE(
			            SUM(f_Leads[Leads]),
			            USERELATIONSHIP('d_Calendário'[Date], f_Leads[Date]),
			            tblWeek
			        ),
			        COUNTROWS(tblWeek),
			        0
			    ),
			    0
			)
			VAR monthBase = MONTH(MAX('d_Calendário'[Date]))
			VAR yearBase = YEAR(MAX('d_Calendário'[Date]))
			VAR tblThisMonth = FILTER(
			    ALL('d_CalendárioApoio'),
			    'd_CalendárioApoio'[Mês]=monthBase && 'd_CalendárioApoio'[Ano]=yearBase
			)
			VAR fullMonthProp = COUNTROWS(FILTER(tblThisMonth, [SemanaOrFDS]="Semana")) + (COUNTROWS(FILTER(tblThisMonth,[SemanaOrFDS]="FDS")) * weekendFactor)
			
			RETURN DIVIDE(
			    IF(
			        weekdayOrWeekend="FDS",
			        weekendFactor,
			        1
			    ),
			    fullMonthProp,
			    0
			)*_Medidas[Meta ( Leads | Periodo | N )]
		formatString: #,0
		lineageTag: 9c576db1-753d-43af-ab7b-f8543add9ba5

		changedProperty = FormatString

