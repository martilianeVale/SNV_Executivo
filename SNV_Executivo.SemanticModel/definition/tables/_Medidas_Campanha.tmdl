	measure 'Dif ( ROI vs Controle )' =
			
			
			VAR selectedDay = MAX('d_Calendário'[Dia Sem.])
			VAR ControlPeriod = IF(
			    selectedDay = "dom" || selectedDay = "sáb",
			    "Controle FDS",
			    "Controle DU"
			)
			
			VAR Margem = CALCULATE(
			        [Receita ( Total | Período 1 | N,n )],
			        FILTER(
			            ALL('d_Calendário'),
			            'd_Calendário'[Periodo Controle]=ControlPeriod
			        )
			    )*0.3460
			
			VAR CustoPerformance = CALCULATE(
			        _Medidas[Investimento Realizado ( Total | Período 1 | N,n )],
			        FILTER(
			            ALL('d_Calendário'),
			            'd_Calendário'[Periodo Controle]=ControlPeriod
			        )
			    )
			
			RETURN DIVIDE(
			    [ROI],
			    DIVIDE((Margem-CustoPerformance),CustoPerformance,0),
			    0
			)-1
		formatString: #,0%;-#,0%;#,0%
		lineageTag: a3f85423-8d40-4e1f-b1d1-40bc030ad762

		changedProperty = FormatString

