	measure 'Dif ( Clicks vs Controle )' =
			
			
			VAR selectedDay = MAX('d_Calendário'[Dia Sem.])
			VAR ControlPeriod = IF(
			    selectedDay = "dom" || selectedDay = "sáb",
			    "Controle FDS",
			    "Controle DU"
			)
			
			RETURN DIVIDE(
			    [Clicks ( Total | Período 1 | N )],
			    CALCULATE(
			        [Clicks ( Média diária )],
			        FILTER(
			            ALL('d_Calendário'),
			            'd_Calendário'[Periodo Controle]=ControlPeriod
			        )
			    ),
			    0
			)-1
